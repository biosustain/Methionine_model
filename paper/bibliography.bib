@article{tsujino_structure_1989,
	title = {Structure of the gene encoding the muscle-specific subunit of human
	         phosphoglycerate mutase.},
	volume = {264},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/2549058},
	abstract = {We report the isolation and analysis of genomic clones
	            containing the entire gene encoding the muscle-specific subunit
	            of human phosphoglycerate mutase. The gene spans 2.83 kilobase
	            pairs and has a three-exon/two-intron structure that is similar
