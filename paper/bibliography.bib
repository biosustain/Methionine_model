@article{rife_kinetic_1980,
	title = {Kinetic mechanism of glutamate dehydrogenase.},
	volume = {19},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/7190024},
	abstract = {Initial velocity patterns and dead-end inhibition studies with
	            oxalylglycine suggest that the addition of NADPH, keto acid, and
	            ammonia occurs with obligatory order. For monocarboxylic keto
	            acids, the keto acid-ammonia initial velocity pattern is
	            equilibrium ordered because koff is much greater than Vmax.
	            alpha-Ketoglutarate gives substrate inhibition that is
	            uncompetitive vs. either NADPH or ammonia in the absence of NADP+
	            , but with high NAD+ is noncompetitive vs. NADPH and
	            uncompetitive vs. ammonia. The inhibition is partial on both
	            slopes and intercepts, showing that ketoglutarate slows down but
