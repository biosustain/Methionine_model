@article{stone_loss_2018,
	title = {Loss of pyruvate kinase {M2} limits growth and triggers innate
	         immune signaling in endothelial cells.},
	volume = {9},
	issn = {2041-1723},
	url = {http://www.nature.com/articles/s41467-018-06406-8},
	doi = {10.1038/s41467-018-06406-8},
	abstract = {Despite their inherent proximity to circulating oxygen and
	            nutrients, endothelial cells (ECs) oxidize only a minor fraction
	            of glucose in mitochondria, a metabolic specialization that is
	            poorly understood. Here we show that the glycolytic enzyme
	            pyruvate kinase M2 (PKM2) limits glucose oxidation, and maintains
	            the growth and epigenetic state of ECs. We find that loss of PKM2
