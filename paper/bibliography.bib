}

@article{bonsignore_metabolism_1971,
	title = {Metabolism of human erythrocyte glucose-6-phosphate dehydrogenase},
	volume = {147},
	issn = {00039861},
	url = {https://linkinghub.elsevier.com/retrieve/pii/0003986171904061},
	doi = {10.1016/0003-9861(71)90406-1},
	abstract = {Human erythrocyte glucose-6-phosphate dehydrogenase exists in
	            two catalytically active forms, i.e., tetramers and dimers, and
	            also monomers which are intrinsically devoid of activity. Several
	            factors are involved in the interconversion of the three species.
	            High pH and ionic strength favor dissociation of the tetramer to
	            dimers, while low values of pH and ionic strength and in addition
	            some divalent cations shift the equilibrium toward the tetramers.
	            NADPH2, formed during the operation of glucose-6-P dehydrogenase,
	            inactivates the enzyme through a specific disaggregation which
	            affects only the dimeric form. Dissociation of the tetramer to
	            inactive monomers can, however, occur through a compulsory
	            mechanism involving the previous conversion to dimers which are
	            themselves disaggregated by NADPH2. Therefore, when the enzyme is
	            working in a system where NADPH2 is not immediately reoxidized
	            (i.e., in the absence of glutathione reductase), a sequential
	            tetramer ⇀ dimer ⇀ monomer conversion may occur as a result of
	            the enzymatic activity.},
	number = {2},
	urldate = {2019-03-05},
	journal = {Archives of Biochemistry and Biophysics},
	author = {Bonsignore, A. and Cancedda, R. and Nicolini, A. and Damiani, G.
	          and De Flora, A.},
	month = dec,
	year = {1971},
	keywords = {G6PDH},
	pages = {493--501},
}

