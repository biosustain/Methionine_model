@article{reed_structural_1996,
	title = {Structural and mechanistic studies of enolase.},
	volume = {6},
	url = {http://dx.doi.org/10.1016/S0959-440X(96)80002-9},
	doi = {10.1016/S0959-440X(96)80002-9},
	abstract = {The high-resolution structure of yeast enolase cocrystallized
