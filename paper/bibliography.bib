@article{gelpi_kinetic_1992,
	title = {Kinetic studies of the regulation of mitochondrial malate
	         dehydrogenase by citrate.},
	volume = {283 ( Pt 1)},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/1567375},
	abstract = {Mitochondrial malate dehydrogenase shows a complex regulation
	            pattern in the presence of citrate. Previously published results
	            indicate that this enzyme is activated by citrate in the
	            NAD(+)----NADH direction and inhibited in the opposite direction.
	            Moreover, high concentrations of L-malate or oxaloacetate produce
	            deviations from the Michaelis-Menten behaviour. Results reported
	            in this paper clearly show that citrate both activates and
	            inhibits mitochondrial malate dehydrogenase in the same direction
	            (NAD(+)----NADH), and in the same reaction medium, depending on
	            substrate concentration. This surprising effect has made it
	            necessary to propose a new kinetic mechanism that extends those
	            previously suggested and allows us to explain both the citrate
	            effect (activating or inhibitory) and the effect of high
	            concentrations of L-malate and oxaloacetate.},
	urldate = {2019-04-01},
	journal = {The Biochemical Journal},
	author = {Gelpí, J L and Dordal, A and Montserrat, J and Mazo, A and Cortés,
	          A},
	month = apr,
